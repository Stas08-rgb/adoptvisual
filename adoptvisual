Spawn.MouseButton1Click:Connect(function()
    local petName = NameBox.Text
    if petName == "" then
        local originalText = Spawn.Text
        Spawn.Text = "üíî Enter name!"
        wait(1)
        Spawn.Text = originalText
        return
    end
    
    local originalSize = Spawn.Size
    local originalText = Spawn.Text
    
    Spawn.Text = "üíñ Creating..."
    local tweenService = game:GetService("TweenService")
    
    tweenService:Create(
        Spawn,
        TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {Size = UDim2.new(0, 190, 0, 45)}
    ):Play()
    
    wait(0.1)
    
    tweenService:Create(
        Spawn,
        TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {Size = originalSize}
    ):Play()
    
    duplicatePet(petName)
    
    Spawn.Text = "üíù Success! üíñ"
    wait(0.5)
    Spawn.Text = originalText
end)

coroutine.wrap(function()
    while wait(0.06) do
        Heart1.Rotation = Heart1.Rotation + 0.8
        Heart2.Rotation = Heart2.Rotation - 1
        Heart3.Rotation = Heart3.Rotation + 1.2
        
        local time = tick()
        local pulse = math.sin(time * 2) * 0.1 + 1
        
        Heart1.Size = UDim2.new(0, 35 * pulse, 0, 35 * pulse)
        Heart2.Size = UDim2.new(0, 30 * pulse, 0, 30 * pulse)
        Heart3.Size = UDim2.new(0, 25 * pulse, 0, 25 * pulse)
        
        local r = math.random()
        if r < 0.3 then
            HeartBorderTop.BackgroundColor3 = Color3.fromRGB(255, 20, 147)
            HeartBorderBottom.BackgroundColor3 = Color3.fromRGB(255, 105, 180)
        elseif r < 0.6 then
            HeartBorderTop.BackgroundColor3 = Color3.fromRGB(255, 105, 180)
            HeartBorderBottom.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
        else
            HeartBorderTop.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
            HeartBorderBottom.BackgroundColor3 = Color3.fromRGB(255, 20, 147)
        end
    end
end)()

local UIS = game:GetService('UserInputService')
local frame = Main
local dragToggle = nil
local dragSpeed = 0.25
local dragStart = nil
local startPos = nil

local function updateInput(input)
	local delta = input.Position - dragStart
	local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
		startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
end

frame.InputBegan:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
		dragToggle = true
		dragStart = input.Position
		startPos = frame.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragToggle = false
			end
		end)
	end
end)

UIS.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		if dragToggle then
			updateInput(input)
		end
	end
end)

print("Valentine pet spawner GUI loaded successfully! üíù")
